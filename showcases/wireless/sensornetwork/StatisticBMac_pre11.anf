            <script>
                <![CDATA[import math
import numpy as np
import pandas as pd
from omnetpp.scave import results, chart, plot, utils
import matplotlib.pyplot as plt

# get chart properties
props = chart.get_properties()
utils.preconfigure_plot(props)

# collect parameters for query
filter_expression = props["filter"]

# query scalar data into dataframe
try:
    df = results.get_scalars(filter_expression, include_attrs=True, include_itervars=True, include_runattrs=True)
except ValueError as e:
    plot.set_warning("Error while querying results: " + str(e))
    exit(1)

if df.empty:
    plot.set_warning("The result filter returned no data.")
    exit(1)
    
print("\ndf\n--\n",df)

# average repetitions    
df = df.groupby(['name','configname','module','iterationvars','inifile'], as_index=False).mean()
print("\ndf after averaging:\n-------------------",df)

# ----------------------------------------------
# get best iterationvars
print("\nbest iterationvars:\n-------------------\n")
# b mac
df_b = df.loc[df['configname'] == 'StatisticBMac']
#print("df_b",df_b)
bmac_max = df_b.value.max()
#print("bmac_max",bmac_max)
best_b = df_b.loc[df_b['value'] == bmac_max, ['iterationvars']]
bestb = df_b.loc[df_b['value'] == bmac_max, ['iterationvars','configname']]
best_b = best_b['iterationvars'].iloc[0]
print("best bmac itervars - just the value\n",best_b)
print("best bmac itervars\n", bestb)

# x mac
df_x = df.loc[df['configname'] == 'StatisticXMac']
#print("df_x",df_x)
xmac_max = df_x.value.max()
#print("xmac_max",xmac_max)
best_x = df_x.loc[df_x['value'] == xmac_max, ['iterationvars']]
bestx = df_x.loc[df_x['value'] == xmac_max, ['iterationvars','configname']]
best_x = best_x['iterationvars'].iloc[0]
print("best xmac itervars - just the value\n",best_x)
print("best xmac itervars\n", bestx)

# l mac
df_l = df.loc[df['configname'] == 'StatisticLMac']
#print("df_l",df_l)
lmac_max = df_l.value.max()
#print("lmac_max",lmac_max)
best_l = df_l.loc[df_l['value'] == lmac_max, ['iterationvars']]
bestl = df_l.loc[df_l['value'] == lmac_max, ['iterationvars','configname']]
best_l = best_l['iterationvars'].iloc[0]
print("best lmac itervars - just the value\n",best_l)
print("best lmac itervars\n", bestl)
# ----------------------------------------------

dfb = df.loc[df['configname'] == 'StatisticBMac']
dfB = dfb.loc[dfb['iterationvars'] == bestb['iterationvars'].iloc[0]]
print("\nbest bmac run\n-------------\n",dfB)

dfx = df.loc[df['configname'] == 'StatisticXMac']
dfX = dfx.loc[dfx['iterationvars'] == bestx['iterationvars'].iloc[0]]
print("\nbest xmac run\n-------------\n",dfX)

dfl = df.loc[df['configname'] == 'StatisticLMac']
dfL = dfl.loc[dfl['iterationvars'] == bestl['iterationvars'].iloc[0]]
print("\nbest lmac run\n-------------\n",dfL)

dfB = dfB.groupby(['name','iterationvars','configname','inifile'], as_index=False).sum()
print("\nbmac residual sum\n----------------\n",dfB)

dfX = dfX.groupby(['name','iterationvars','configname','inifile'], as_index=False).sum()
print("\nxmac residual sum\n----------------\n",dfX)

dfL = dfL.groupby(['name','iterationvars','configname','inifile'], as_index=False).sum()
print("\nlmac residual sum\n----------------\n",dfL)

print("dfb-------------------------")

print(dfB['name'])

# per-packet power consumption

"""
print('W\n')
x = (dfB.loc[dfB['name'] == 'packetReceived:count', ['value']])
print(x)
print(x['value'].iloc[0])
print("WW\n")
x = (dfB.loc[dfB['name'] == 'residualEnergyCapacity:last', ['value']])
print(x)


x = dfB.loc[dfB['name'] == 'packetReceived:count', ['value']]
x['value'].iloc[0]

x = dfB.loc[dfB['name'] == 'packetReceived:count', ['value']]['value'].iloc[0]
#x['value'].iloc[0]
print("fuck this",x)
"""

dfB['pppc'] = dfB.loc[dfB['name'] == 'residualEnergyCapacity:last', ['value']]['value'].iloc[0]/dfB.loc[dfB['name'] == 'packetReceived:count', ['value']]['value'].iloc[0]
print("\nbmac per-packet power consumption\n---------------\n",dfB)

dfX['pppc'] = dfX.loc[dfX['name'] == 'residualEnergyCapacity:last', ['value']]['value'].iloc[0]/dfX.loc[dfX['name'] == 'packetReceived:count', ['value']]['value'].iloc[0]
print("\nxmac per-packet power consumption\n---------------\n",dfX)

dfL['pppc'] = dfL.loc[dfL['name'] == 'residualEnergyCapacity:last', ['value']]['value'].iloc[0]/dfL.loc[dfL['name'] == 'packetReceived:count', ['value']]['value'].iloc[0]
print("\nlmac per-packet power consumption\n---------------\n",dfL)

DF = dfB
DF = DF.append(dfX)
DF = DF.append(dfL)
print("\nDF\n",DF)

# now we can plot multiple stuff

DF['value'] = DF['pppc']
df = DF.loc[DF['name'] == 'residualEnergyCapacity:last', ['name','configname','inifile', 'value']]
df['value'] *= -1
print("residual",df)

##dfx = df.loc[df['configname'] == 'StatisticXMac']
##dfX = dfx.loc[dfx['iterationvars'] == bestx['iterationvars'].iloc[0]]
##print("\nbest xmac run\n-------------\n",dfX)

##dfb = df.loc[df['configname'] == 'StatisticBMac']
##dfB = dfb.loc[dfb['iterationvars'] == bestb['iterationvars'].iloc[0]]
##print("\nbest bmac run\n-------------\n",dfB)

#print("X",df[['iterationvars','configname']])

#dfallbest = df.loc[df[['iterationvars','configname']] == bestb]
#print("dfallbest\n",dfallbest)

groups = props["groups"].split(",")
series = props["series"].split(",")

if not groups[0] and not series[0]:
    print("The Groups and Series options were not set in the dialog, inferring them from the data.")
    g, s = ("module", "name") if len(df) == 1 else utils.pick_two_columns(df)
    groups, series = [g], [s]

if not groups[0] or not series[0]:
    plot.set_warning("Please set both the Groups and Series properties in the dialog - or neither, for automatic setup.")
    exit(1)

utils.assert_columns_exist(df, groups + series, "No such iteration variable or run attribute")

for c in groups + series:
    df[c] = pd.to_numeric(df[c], errors="ignore")

df.sort_values(by=groups+series, axis='index', inplace=True)

df['configname'] = pd.Categorical(df['configname'], ["StatisticBMac",'StatisticXMac','StatisticLMac'])

df = df.replace(['StatisticBMac'],'BMac')
df = df.replace(['StatisticXMac'],'XMac')
df = df.replace(['StatisticLMac'],'LMac')

names = utils.get_names_for_title(df, props)

confidence_level_str = props["confidence_level"] if "confidence_level" in props else "none"

if confidence_level_str == "none":
    df = pd.pivot_table(df, index=groups, columns=series, values='value')
    utils.plot_bars(df, props, names)
else:
    confidence_level = float(confidence_level_str[:-1])/100
    conf_int = lambda values: utils.confidence_interval(confidence_level, values) if len(values) > 1 else math.nan
    df = pd.pivot_table(df, index=groups, columns=series, values='value', aggfunc=[np.mean, conf_int], dropna=False)
    utils.plot_bars(df["mean"], props, names, df["<lambda>"])

utils.postconfigure_plot(props)

plt.title("Power consumption per packet")
plt.grid(axis='y', linestyle='--')
plt.xlabel('')
plt.ylabel('Power consumption  [J]')

ax = plt.gca()
for p in ax.patches:
    if p.get_height() > 0.01:
        ax.annotate(str(round(p.get_height(),4)), (p.get_x() + p.get_width() / 2, p.get_height() - 0.002),
                    horizontalalignment='center', verticalalignment='top', color='white', size=10, zorder=12)
    else:
        ax.annotate(str(round(p.get_height(),4)), (p.get_x() + p.get_width() / 2, p.get_height() + 0.005),
                    horizontalalignment='center', verticalalignment='top', color='black', size=10, zorder=12)

plt.savefig('power3.png', dpi=150, transparent=True)

utils.export_image_if_needed(props)
utils.export_data_if_needed(df, props)
]]>
            </script>
