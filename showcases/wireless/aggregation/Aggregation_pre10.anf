            <property name="filter" value="type =~ scalar AND isfield =~ false AND module =~ &quot;*.host2.*&quot; AND name =~ packetReceived:count&#10;AND NOT (type =~ scalar AND runattr:replication =~ &quot;#0&quot; AND module =~ &quot;AggregationShowcase.host2.udp&quot; AND name =~ packetReceived:count AND (&#10;    (runattr:experiment =~ Aggregation) OR&#10;    (runattr:experiment =~ NoAggregation) OR&#10;    (runattr:experiment =~ VoicePriorityAggregation)&#10;))"/>
            <property name="series" value="inifile"/>
            <property name="plt.style" value="default"/>
            <property name="legend_show" value="false"/>
            <property name="matplotlibrc.legend.shadow" value="false"/>
